    "del": "*",
    "es6-promise": "^3.2.1",
    "gulp": "*",
    "gulp-autoprefixer": "*",
    "gulp-concat": "*",
    "gulp-connect": "*",
    "gulp-livereload": "*",
    "gulp-load-plugins": "*",
    "gulp-sass": "*",
    "gulp-size": "*",
    "gulp-uglify": "*",
    "gulp-util": "*",
    "gulp-watch": "*",
    "gulp-webpack": "*",
    "jest-cli": "*",
    "object-assign": "*",
    "react-tools": "*",
    "webpack": "*"
  },
  "dependencies": {
    "alt": "*",
    "babel-preset-react": "^6.11.1",
    "classnames": "^1.2.2",
    "immutable": "*",
    "react": "*",
    "react-dnd": "*",
